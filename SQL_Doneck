SELECT dr.* , pp.ppSerNum_Cor as ppp, pp.ppManu, pp.ppBran, pp.ppInfX, pp.ppNamC, pp.ppSerN_iMod, sum(dr.dVwrk)
from gmp.daily_reports as dr
left join gmp.pp_reports as pp
on dr.serNUM = pp.ppSerNum_Cor
where dr.typeDEV != 23 and dr.typeDEV != 24
group by dr.serNUM
; не правильная сумма